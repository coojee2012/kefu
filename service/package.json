{
  "name": "web_server",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "npm run build-ts",
    "dev": "supervisor -w dist ./dist/app.js",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"nodemon dist/app.js\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "private": true,
  "dependencies": {
    "async": "^2.4.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "express-ejs-layouts": "^2.3.1",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "express-winston": "^2.4.0",
    "favicon": "0.0.2",
    "injection-js": "^2.1.0",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "logger": "0.0.1",
    "method-override": "^2.3.8",
    "mongoose": "^4.8.1",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "session": "^0.1.0",
    "slug": "^0.9.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.52",
    "@types/mongodb": "^2.2.2",
    "@types/mongoose": "^4.7.5",
    "@types/node": "^7.0.5",
    "@types/redis": "^2.6.0",
    "concurrently": "^3.4.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.3",
    "merge2": "^1.0.3",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
